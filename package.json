{
  "name": "@crystallize/import-utilities",
  "version": "0.14.0",
  "main": "dist/index.js",
  "author": "Brendan Goodenough <brendan@goodenough.nz>",
  "files": [
    "dist/**/*"
  ],
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc -p . --declaration",
    "test": "ava",
    "dev:bootstrapper": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"node -r ts-node/register src/bootstrap-tenant/__dev.ts\"",
    "dev:bootstrapper:inspect": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"node --inspect-brk -r ts-node/register src/bootstrap-tenant/__dev.ts\""
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "json-to-graphql-query": "^2.0.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@types/node": "^15.6.1",
    "@types/node-fetch": "^2.5.10",
    "ava": "^3.15.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}